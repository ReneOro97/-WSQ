#include <iostream>
using namespace std;

long fibonacci (long n){
	int A = 1, B=1, C;
	for (int i =2; i<n; i++)
	{
		C=A+B;
		A=B;
		B=C;
		
		
		
	}
	return C;
}
	
	int main (){
		int n;
		cout <<"Choose a number and I would give you the value of that number in Fibonacci sequence: "; cin >> n;
		cout << "The value of the "<< n<< "Â° number in Fibonacci sequence is: " << endl;
		cout << fibonacci(n) << endl;
	}
